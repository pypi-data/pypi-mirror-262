{
  "name": "yfiles-jupyter-graphs",
  "version": "1.6.2",
  "description": "A diagram visualization extension for JupyterLab and Jupyter Notebooks powered by yFiles for HTML",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "widgets",
    "yfiles",
    "visualization",
    "graph",
    "diagrams"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js",
    "css/*.css"
  ],
  "private": "true",
  "homepage": "https://www.yworks.com/products/yfiles-graphs-for-jupyter",
  "bugs": {
    "url": "https://github.com/yWorks/yfiles-jupyter-graphs/issues"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "author": {
    "name": "yWorks Support Team",
    "email": "yfileshtml@yworks.com"
  },
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/yWorks/yfiles-jupyter-graphs"
  },
  "scripts": {
    "create-wheel": "yarn run clean && python -m build",
    "build": "yarn run build:lib && yarn run build:nbextension:dev && yarn run build:labextension",
    "build:prod": "yarn run build:lib && yarn run build:nbextension && yarn run build:labextension && yarn run postprocess",
    "build:labextension": "jupyter labextension build .",
    "build:lib": "tsc && node -e \"require('fs').copyFileSync('./src/workers/layout.worker.js', 'lib/workers/layout.worker.js');\"",
    "build:nbextension": "webpack --mode=production",
    "build:nbextension:dev": "webpack --mode=development",
    "clean": "yarn run clean:lib && yarn run clean:nbextension && yarn run clean:labextension",
    "clean:lib": "rimraf lib",
    "clean:labextension": "rimraf yfiles_jupyter_graphs/labextension",
    "clean:nbextension": "rimraf \"yfiles_jupyter_graphs/nbextension/!(extension.js)\"",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx",
    "local:build": "webpack --config webpack.local.build.config.js",
    "local:watch": "webpack serve --config webpack.local.build.config.js --open",
    "prepack": "yarn run build:lib",
    "test": "jest",
    "watch": "npm-run-all -p watch:*",
    "watch:lib": "tsc -w",
    "watch:nbextension": "webpack --watch",
    "watch:labextension": "jupyter labextension watch .",
    "postprocess": "grunt post-build && cd ./umd/ && npm pack"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^3.4.1",
    "@jupyter-widgets/base": "^1.1.10 || ^2.0.0 || ^3.0.0 || ^4.0.0 || ^5.0.0 || ^6.0.0",
    "@mdi/js": "^6.5.95",
    "vue": "^2.6.14",
    "vue-json-viewer": "^2.2.19",
    "yfiles": "../yfiles-26.0.3-jupyter-widget.tgz"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@jupyterlab/builder": "^3.0.0",
    "@phosphor/application": "^1.6.0",
    "@phosphor/widgets": "^1.6.0",
    "@types/jest": "^26.0.0",
    "@types/resize-observer-browser": "^0.1.6",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "@yworks/optimizer": "^1.8.1",
    "acorn": "^7.2.0",
    "css-loader": "^6.5.1",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fs-extra": "^7.0.0",
    "grunt": "^1.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.0",
    "load-grunt-tasks": "^5.1.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^2.0.5",
    "rimraf": "^2.6.2",
    "source-map-loader": "^1.1.3",
    "style-loader": "^1.0.0",
    "ts-jest": "^26.0.0",
    "ts-loader": "^8.0.0",
    "typescript": "~4.5.5",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0",
    "webpack-license-plugin": "^4.2.1",
    "worker-loader": "^3.0.8"
  },
  "jupyterlab": {
    "extension": "lib/plugin",
    "webpackConfig": "./webpack.labextension.config.js",
    "outputDir": "yfiles_jupyter_graphs/labextension/",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      }
    },
    "_build": {
      "load": "static/remoteEntry.ad63ff24e3e1da2b5fca.js",
      "extension": "./extension"
    }
  }
}
